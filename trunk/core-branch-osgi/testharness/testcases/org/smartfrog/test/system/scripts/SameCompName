#start the daemon in a seperate window in localhost
sfDaemon &
sleep 1
#open two windows and deploy same component
sfStart localhost ex1 org/smartfrog/test/system/scripts/tcn14.sf
sfStart localhost ex1 org/smartfrog/test/system/scripts/tcn14.sf
#stop the component
sfStop localhost ex1
#stop the daemon
sfStopDaemon localhost

