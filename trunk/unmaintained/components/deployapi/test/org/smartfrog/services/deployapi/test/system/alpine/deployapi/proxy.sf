#include "/org/smartfrog/services/utils/setproperty/components.sf"

/**
 * This is the settings for the server test for steve to do the ports

 */
  Proxy extends SystemProperties {
    enabled true;
    setOnDeploy enabled;
    unsetOnTerminate false;
    host "web-proxy";
    port "8088";
    properties [
      ["http.proxyHost",host],
      ["http.proxyPort",port],
      ["https.proxyHost",host],
      ["https.proxyPort",port]
      ];
  }