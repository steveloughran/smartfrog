      <packages>org.codehaus.activesoap.handler:org.codehaus.activesoap.handler.*</packages>
    </packageGroup>
    <packageGroup>
      <title>SOAP version details</title>
      <packages>org.codehaus.activesoap.soap</packages>
    </packageGroup>
    <packageGroup>
      <title>Utility classes</title>
      <packages>org.codehaus.activesoap.util</packages>
    </packageGroup>
  </packageGroups>

