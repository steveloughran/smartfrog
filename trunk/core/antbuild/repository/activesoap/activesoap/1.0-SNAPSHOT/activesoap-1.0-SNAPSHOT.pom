      <title>Core ActiveSOAP API</title>
      <packages>org.codehaus.activesoap</packages>
    </packageGroup>
    <packageGroup>
      <title>WS-Addressing API and implementation</title>
      <packages>org.codehaus.activesoap.policy.addressing:org.codehaus.activesoap.policy.addressing.*</packages>
