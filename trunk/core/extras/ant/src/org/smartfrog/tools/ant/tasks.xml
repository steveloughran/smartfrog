<?xml version="1.0"?>
<antlib>
    <!-- ant1.6+ antlib declaration for the classes
    use with the declaration xmlns:axis="antlib:org.smartfrog.tools.ant.tasks" to
    trigger ant's autoload of this file, provided the relevant jars
    are in the antlib directory, or a dir stated with -lib on the command line"
    Alternatively, load with something like
    
    <typedef uri="http://smartfrog.org/"
        resource="/org/smartfrog/tools/ant/tasks.xml"
        classpathref="smartfrog.classpath"
        xmlns:sf="http://smartfrog.org/"
    -->
    <typedef name="sf-parse" classname="org.smartfrog.tools.ant.Parse"/>
    <typedef name="sf-undeploy" classname="org.smartfrog.tools.ant.StopApplication"/>
    <typedef name="sf-stopdaemon" classname="org.smartfrog.tools.ant.StopDaemon"/>
    <typedef name="sf-startdaemon" classname="org.smartfrog.tools.ant.StartDaemon"/>
    <typedef name="sf-deploy" classname="org.smartfrog.tools.ant.StartApplication"/>
<!--
    <typedef name="" classname="org.smartfrog.tools.ant."/>
    <typedef name="" classname="org.smartfrog.tools.ant."/>

 -->
</antlib>
