mv -v -T sf-quartz-*.jar sf-quartz.jar
mv -v -T sf-vmware*.jar sf-vmware.jar
mv -v -T sf-xmpp*.jar sf-xmpp.jar
mv -v -T sf-ant*.jar sf-ant.jar
mv -v -T sf-anubis*.jar sf-anubis.jar
mv -v -T sf-installer*.jar sf-installer.jar
mv -v -T sf-jmx*.jar sf-jmx.jar
mv -v -T sf-loggingservices*.jar sf-loggingservices.jar
mv -v -T sf-net*.jar sf-net.jar
mv -v -T sf-ssh*.jar sf-ssh.jar
mv -v -T sf-rest*.jar sf-rest.jar
mv -v -T quartz*.jar quartz.jar
#mv -v -T sf-avalanche-core*.jar sf-avalanche-core.jar
mv -v -T je-*.jar je.jar
mv -v -T smackx-*.jar smackx.jar
mv -v -T smack-*.jar smack.jar
mv -v -T sf-emailer*.jar sf-emailer.jar
mv -v -T mail*.jar mail.jar
