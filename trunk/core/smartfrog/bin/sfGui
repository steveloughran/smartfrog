#! /bin/sh
if ! test $SFHOME; 
then 
    export SFHOME=$PWD;
    echo SFHOME undefined, using $SFHOME as base directory;
fi

# for i in $SFHOME/lib/*.jar; do CLASSPATH=$i:$CLASSPATH; done
# export CLASSPATH
. $SFHOME/bin/setSFProperties

PATH=:./bin:$PATH
JAVA_HEAP_SIZE=32

echo $CLASSPATH

java -cp ./src:$CLASSPATH org.smartfrog.tools.gui.browser.SFGui "$@"
