if ! test $SFHOME; then export SFHOME=$PWD; fi
if [ $# -ne 3 ];
then
echo "Insufficient arguments to use sfStart"
echo "Usage: sfStart HostName ApplicationName URL"
exit
fi
for i in $SFHOME/lib/*.jar; do CLASSPATH=$i:$CLASSPATH; done
export CLASSPATH

java -Dorg.smartfrog.iniFile=$SFHOME/bin/default.ini org.smartfrog.SFSystem -a $2:DEPLOY:$3::$1: -e

