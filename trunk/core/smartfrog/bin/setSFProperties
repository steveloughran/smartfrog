#!/bin/sh

# general properties
export SERVER=localhost:8080
# Please edit codebase if you have any other jar file in webserver 
export CODEBASE="http://$SERVER/sfExamples.jar" 

#------------------------------------------------------
# SF ENV PROPERTIES  - Please edit with your preferences
# ------------------------------------------------------

if ! test $SFDEFAULTINI; then
    export SFDEFAULTINI="-Dorg.smartfrog.iniFile=org/smartfrog/default.ini"
fi
if ! test $SFDEFAULTSF; then
    export SFDEFAULTSF="-Dorg.smartfrog.sfcore.processcompound.sfDefault.sfDefault=org/smartfrog/default.sf"
fi
# if ! test $SFSECURITY;then
#export SFSECURITY="-Djava.security.policy==$SFHOME/private/sf.policy -Djava.security.manager -Dorg.smartfrog.sfcore.security.keyStoreName=$SFPRIVATE/mykeys.st -Dorg.smartfrog.sfcore.security.propFile=$SFPRIVATE/SFSecurity.properties"
#fi
# if ! test $SFCODEBASE;then
#export SFCODEBASE="-Dorg.smartfrog.codebase=$CODEBASE"
#fi

# ------------------------------------------------------

export SFCMDPARAMETERS=

if test $SFSECURITY; then
 	export SFCMDPARAMETERS="$SFCMDPARAMETERS $SFSECURITY"
fi
if test $SFDEFAULTINI; then
	export SFCMDPARAMETERS="$SFCMDPARAMETERS $SFDEFAULTINI"
fi
if test $SFCODEBASE; then
	export SFCMDPARAMETERS="$SFCMDPARAMETERS $SFCODEBASE"
fi

