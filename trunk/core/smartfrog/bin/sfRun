#! /bin/sh
if ! test $SFHOME; then export SFHOME=$PWD; fi
if [ -z $1 ] ; then
echo "Insufficent arguments to use sfRun"
echo "Usage: sfRun URL [-e]"
exit
fi 
if [ $1 == "-?" ];then
echo "Usage: sfRun URL [-e]"
exit
fi 
for i in $SFHOME/lib/*.jar; do CLASSPATH=$i:$CLASSPATH; done
export CLASSPATH

. $SFHOME/bin/setSFProperties

if test $SFDEFAULTiSF; then
	export SFCMDPARAMETERS="$SFCMDPARAMETERS $SFDEFAULTSF"
fi

java $SFCMDPARAMETERS org.smartfrog.SFSystem -a :DEPLOY:$1::: $2
