#!/bin/bash
export TOP=$PWD
export DEST=$1
export RELNAME=$2
shift
shift
count=0
for x in $@;
  do 
   	if [ -e "$DEST/daemons/$x/$RELNAME/dist/daemon.out" ]; then
    		 count=$((count + `grep -c "SFStart" $DEST/daemons/$x/$RELNAME/dist/daemon.out`))
	fi
 done
if [ $count  -eq 4 ]; then
	echo TEST PASSED > report
else
	echo TEST FAILED > report
fi


