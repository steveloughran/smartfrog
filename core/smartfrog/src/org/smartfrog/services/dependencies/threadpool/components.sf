
#include "org/smartfrog/components.sf"

/*
 * A threadpool manages a pool of threads to handle tasks
 */
ThreadPool extends Prim {
    sfClass "com.hp.ov.cc.threadpool.ThreadPoolImpl";
    sfExport false;

    maxThreads 20;
    maxFreeThreads 10;
}

/*
 * A component to test the thread pool
 */
ThreadPoolPTester extends Prim {
    sfClass "com.hp.ov.cc.threadpool.ThreadPoolTester";
    sfExport false;

    threadPool TBD;
}
