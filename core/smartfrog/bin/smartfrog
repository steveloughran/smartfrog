if ! test $SFHOME; then export SFHOME=$PWD; fi
export cmdSFSystem=java -Dorg.smartfrog.iniFile=$SFHOME/bin/default.ini org.smartfrog.SFSystem
if [ -z $1 ] ; then
echo "Insufficent arguments to use smartfrog"
$cmdSFSystem 
exit
fi 
for i in $SFHOME/lib/*.jar; do CLASSPATH=$i:$CLASSPATH; done
export CLASSPATH
$cmdSFSystem $*