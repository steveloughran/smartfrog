#! /bin/sh
if ! test "$SFHOME"; then export SFHOME=$PWD; fi


#for i in $SFHOME/lib/*.jar; do CLASSPATH=$i:$CLASSPATH; done
#export CLASSPATH

. "$SFHOME/bin/setSFProperties"

#java $SFCMDPARAMETERS org.smartfrog.SFSystem $*
if test "$SFCODEBASE" && ! test "$SFSECURITY"; then
        java "$SFDEFAULTINI" "$SFCODEBASE" org.smartfrog.SFSystem "$@"
elif test "$SFSECURITY" && ! test "$SFCODEBASE"; then
        java "$SFDEFAULTINI" $SFSECURITY org.smartfrog.SFSystem "$@"
elif test "$SFCODEBASE" && test "$SFSECURITY"; then
        java "$SFDEFAULTINI" "$SFCODEBASE" $SFSECURITY org.smartfrog.SFSystem "$@"
else
        java "$SFDEFAULTINI" org.smartfrog.SFSystem "$@"
fi
     
