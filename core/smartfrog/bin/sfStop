if ! test $SFHOME; then export SFHOME=$PWD; fi
if [ $# -ne 2 ];
then
echo "Insufficient arguments to use sfStop"
echo "Usage: sfStop HostName ApplicationName"
exit
fi 
for i in $SFHOME/lib/*.jar; do CLASSPATH=$i:$CLASSPATH; done
export CLASSPATH
java -Dorg.smartfrog.iniFile=$SFHOME/bin/default.ini  org.smartfrog.SFSystem -h $1 -t $2 -e
