if ! test $SFHOME; then export SFHOME=$PWD; fi
if [ $# -ne 0 ];
then
echo "Insufficient argument(s) to use sfDaemon"
echo "Usage: sfDaemon"
exit
fi
for i in $SFHOME/lib/*.jar; do CLASSPATH=$i:$CLASSPATH; done
export CLASSPATH
java -Dorg.smartfrog.iniFile=$SFHOME/bin/default.ini -Dorg.smartfrog.iniSFFile=$SFHOME/bin/default.sf -Dorg.smartfrog.sfcore.processcompound.sfProcessName=rootProcess org.smartfrog.SFSystem $*
