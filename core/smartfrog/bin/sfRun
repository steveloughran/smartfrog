#! /bin/sh
if ! test "$SFHOME"; then export SFHOME=$PWD; fi
if [ -z $1 ] ; then
echo "Insufficent arguments to use sfRun"
echo "Usage: sfRun URL [-e]"
exit
fi 
if [ $1 == "-?" ];then
echo "Usage: sfRun URL [-e]"
exit
fi 
#for i in $SFHOME/lib/*.jar; do CLASSPATH=$i:$CLASSPATH; done
#export CLASSPATH

. "$SFHOME/bin/setSFProperties"

#if test "$SFDEFAULTSF"; then
#	export SFCMDPARAMETERS="$SFCMDPARAMETERS $SFDEFAULTSF"
#fi

#java "$SFCMDPARAMETERS" org.smartfrog.SFSystem -a :DEPLOY:$1::: $2


if test "$SFCODEBASE" && ! test "$SFSECURITY"; then
        java "$SFDEFAULTINI" "$SFDEFAULTSF" "$SFCODEBASE" org.smartfrog.SFSystem -a :DEPLOY:$1::: $2
elif test "$SFSECURITY" && ! test "$SFCODEBASE"; then
        java "$SFDEFAULTINI" "$SFDEFAULTSF" "$SFSECURITY" org.smartfrog.SFSystem -a :DEPLOY:$1::: $2
elif test "$SFCODEBASE" && test "$SFSECURITY"; then
        java "$SFDEFAULTINI" "$SFDEFAULTSF" "$SFCODEBASE" "$SFSECURITY" org.smartfrog.SFSystem -a :DEPLOY:$1::: $2
else 
        java "$SFDEFAULTINI" "$SFDEFAULTSF" org.smartfrog.SFSystem -a :DEPLOY:$1::: $2
fi

