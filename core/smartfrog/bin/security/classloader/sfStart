if ! test $SFHOME; then export SFHOME=$PWD; fi
if ! test $SFPRIVATE; then export SFPRIVATE=$SFHOME/private; fi
if ! test $SFHOSTNAME; then export SFHOSTNAME=host1; fi
if [ $3 -ne 3 ];then
echo "Insufficient argument(s) to use sfStart"
echo "Usage: sfStart HostName ApplicationName URL"
exit
fi
export CLASSPATH=$SFHOME/signedLib/smartfrog.jar:$SFHOME/signedLib/sfServices.jar:$SFHOME/signedLib/sfTestCases.jar:$CLASSPATH
export SERVER=eb97201.india.hp.com:8080
# Please edit codebase if you have any other jar file in webserver
export CODEBASE="http://$SERVER/sfExamples.jar"

java -Djava.security.manager -Djava.security.policy==$SFPRIVATE/sf.policy -Dorg.smartfrog.sfcore.security.keyStoreName=$SFPRIVATE/$SFHOSTNAME/mykeys.st -Dorg.smartfrog.sfcore.security.propFile=$SFPRIVATE/$SFHOSTNAME/SFSecurity.properties -Dorg.smartfrog.codebase=$CODEBASE org.smartfrog.SFSystem -a $2:DEPLOY:$3::$1: -e 
