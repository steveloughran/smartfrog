	FTP:ftpHost "ebnt171.india.hp.com";
	FTP:username "ritu";
        FTP:transferType "put";
	FTP:localFiles ["D:\\temp\\one.txt","D:\\temp\\two.txt"];
	FTP:remoteFiles ["/home/ritu/tmp/test1",
                     "/home/ritu/tmp/test2"];
    }
 }
}
