#!/bin/bash
export TOP=$PWD
export DEST=$1
export RELNAME=$2
shift
shift
count=0
res=1
for x in $@;
  do
        if [ -e "$DEST/daemons/$x/nohup.out" ]; then
                 count=$((count + `grep -c "SFStart" $DEST/daemons/$x/nohup.out`))
                 res=$((res + 1))
        fi
 done
res=$((res * 2))
if [ $count  -eq $res ]; then
        echo TEST PASSED > report
else
        echo TEST FAILED > report
fi



