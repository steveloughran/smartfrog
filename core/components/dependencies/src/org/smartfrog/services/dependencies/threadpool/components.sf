
#include "org/smartfrog/components.sf"

/*
 * A threadpool manages a pool of threads to handle tasks
 * SimpleThreadPool uses java.util.concurrent functionality.
 * Simple fixed limit on thread number.
 */
SimpleThreadPool extends Prim {
    sfClass "org.smartfrog.services.dependencies.threadpool.SimpleThreadPoolImpl";
    sfExport false;

    numThreads 20;
}
